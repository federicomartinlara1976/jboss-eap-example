{
  "id": "jboss-eap-example",
  "realm": "jboss-eap-example",
  "displayName": "JBoss EAP Example",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "clients": [
    {
      "clientId": "jboss-eap-app",
      "name": "JBoss EAP Application",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "rootUrl": "http://localhost:8080",
      "baseUrl": "/",
      "redirectUris": [
        "http://localhost:8080/*"
      ],
      "webOrigins": [
        "http://localhost:8080"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Usuario est√°ndar"
      },
      {
        "name": "admin",
        "description": "Administrador del sistema"
      }
    ]
  },
  "users": [
    {
      "username": "usuario",
      "email": "usuario@example.com",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ],
      "realmRoles": [
        "user"
      ]
    },
    {
      "username": "admin",
      "email": "admin@example.com",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "admin"
        }
      ],
      "realmRoles": [
        "user",
        "admin"
      ]
    }
  ]
}
